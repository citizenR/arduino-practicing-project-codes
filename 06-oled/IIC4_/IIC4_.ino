void draw(void) {
  // graphic commands to redraw the complete screen should be placed here
  switch (page) {
    case 1:  u8g.drawXBMP( 0, 0, 128, 64, yingjitongxun1);  break;
    case 2:  u8g.drawXBMP( 0, 0, 64, 64, bh4mfe2);  break;
    case 3:  u8g.drawXBMP( 0, 0, 32, 32, zhengnengliang3);  break;
    case 4:  u8g.drawXBMP( 0, 0, 16, 16, yuanguang4);  break;
    case 5:  u8g.drawXBMP( 0, 0, 128, 64, wudeng5);  break;
    case 6:  u8g.drawXBMP( 0, 0, 128, 64, mingdi6);  break;
    case 7:  u8g.drawXBMP( 0, 0, 128, 64, gps7);  break;
  }
  Serial.println(page);
}

void setup(void) {
  Serial.begin(9600);
}
void loop(void) {
  // picture loop
  u8g.firstPage();
  do {
    draw();
  } while ( u8g.nextPage() );

  page == max_pages ? page = 0 : page++;
  delay(2000);
}
